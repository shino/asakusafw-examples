"in_csv"
@directio.csv(
    has_header = FALSE,
    date = "yyyy-MM-dd"
)
in_csv = {
    "key"
    @directio.csv.field(name = "key")
    key : TEXT;

    "value"
    @directio.csv.field(name = "value")
    value : DECIMAL;
};

"out_parq"
@directio.hive.parquet(
    table_name = "out_parq"
)
out_parq = {

    "key"
    @directio.hive.field(name = "key")
    key : TEXT;

    "dec"
    @directio.hive.field(name = "value")
    value : DECIMAL;
};
